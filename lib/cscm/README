>> repos = Cscm::Repository.new("test/adapters/fixtures/subversion/")
=> #<Cscm::Repository:0x249d3a4 @adapter=#<Cscm::Adapters::Subversion::Base:0x249d368 @fs=#<SWIG::TYPE_p_svn_fs_t:0x249d0e8>, @options={}, @repos=#<SWIG::TYPE_p_svn_repos_t:0x249d228>, @repository_path="test/adapters/fixtures/subversion">, @repository_path="test/adapters/fixtures/subversion/">
>> root = repos.node("/")
